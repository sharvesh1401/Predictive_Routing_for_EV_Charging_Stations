# ðŸ”’ Environment Variables for EV Routing Simulation
# Copy this file to .env.local and fill in your actual API keys

# API Configuration
REACT_APP_API_URL=https://ev-routing-backend.vercel.app

# Mapbox API Key (Required for maps)
REACT_APP_MAPBOX_TOKEN=your_mapbox_token_here

# AI API Keys (Required for AI navigation)
REACT_APP_DEEPSEEK_API_KEY=your_deepseek_api_key_here
REACT_APP_GROQ_API_KEY=your_groq_api_key_here

# Optional: OpenAI API Key (Alternative AI provider)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Security Configuration
REACT_APP_SECURITY_LEVEL=maximum
REACT_APP_ENABLE_MONITORING=true
REACT_APP_LOG_LEVEL=info

# Development Configuration
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_ANALYTICS=false

